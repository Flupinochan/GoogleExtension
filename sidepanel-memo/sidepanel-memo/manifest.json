{
  "manifest_version": 3,
  "name": "SidePanelSyncMemo",
  "short_name": "Memo",
  "version": "0.0.1",
  "version_name": "0.0.1",
  "description": "It is a lightweight note-taking app that syncs across Google accounts!",
  "author": "metalmental",
  "key": "",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "24": "images/icon-24.png",
      "32": "images/icon-32.png"
    }
  },
  "side_panel": {
    "default_path": "sidepanel/sidepanel.html"
  },
  "background": {
    "service_worker": "service_worker/service_worker.js",
    "type": "module"
  },
  "permissions": [
    "sidePanel",
    "storage"
  ]
}